
// See https://github.com/julia-vscode/julia-devcontainer/blob/master/Dockerfile for image contents
{
	"name": "Genie",
	"image": "ghcr.io/julia-vscode/julia-devcontainer:latest",

	// Configure tool-specific properties.
	"customizations": {
		// Configure properties specific to VS Code.
		"vscode": {
			// Add the IDs of extensions you want installed when the container is created.
			"extensions": [
				"julialang.language-julia",
				"geniebuilder.geniebuilder"

			]
		}
	},
"features": {
    "ghcr.io/julialang/devcontainer-features/julia:latest": {
	     "channel": "1.9"
    }
},
	"postStartCommand": "startcmd.sh",
	"forwardPorts": [8000]
}
